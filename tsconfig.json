      "@skills/application/*": ["skills/application/*"],
      "@skills/infrastructure/*": ["skills/infrastructure/*"],
      "@skills/modules/*": ["skills/modules/*"],

      "@projects/root/*": ["projects/*"],
      "@projects/domain/*": ["projects/domain/*"],
      "@projects/application/*": ["projects/application/*"],
      "@projects/infrastructure/*": ["projects/infrastructure/*"],
      "@projects/modules/*": ["projects/modules/*"],

      "@presentations/root/*": ["presentations/*"],
      "@presentations/domain/*": ["presentations/domain/*"],
      "@presentations/application/*": ["presentations/application/*"],
      "@presentations/infrastructure/*": ["presentations/infrastructure/*"],
      "@presentations/modules/*": ["presentations/modules/*"],

      "@personals/root/*": ["personals/*"],
      "@personals/domain/*": ["personals/domain/*"],
      "@personals/application/*": ["personals/application/*"],
      "@personals/infrastructure/*": ["personals/infrastructure/*"],
      "@personals/modules/*": ["personals/modules/*"],

      "@experiences/root/*": ["experiences/*"],
      "@experiences/domain/*": ["experiences/domain/*"],
      "@experiences/application/*": ["experiences/application/*"],
      "@experiences/infrastructure/*": ["experiences/infrastructure/*"],
      "@experiences/modules/*": ["experiences/modules/*"],

      "@educations/root/*": ["educations/*"],
      "@educations/domain/*": ["educations/domain/*"],
      "@educations/application/*": ["educations/application/*"],
      "@educations/infrastructure/*": ["educations/infrastructure/*"],
