      "@curriculums/infrastructure/*": ["curriculums/infrastructure/*"],
      "@curriculums/modules/*": ["curriculums/modules/*"],

      "@courses/root/*": ["courses/*"],
      "@courses/domain/*": ["courses/domain/*"],
      "@courses/application/*": ["courses/application/*"],
      "@courses/infrastructure/*": ["courses/infrastructure/*"],
      "@courses/modules/*": ["courses/modules/*"],

      "@certifications/root/*": ["certifications/*"],
      "@certifications/domain/*": ["certifications/domain/*"],
      "@certifications/application/*": ["certifications/application/*"],
      "@certifications/infrastructure/*": ["certifications/infrastructure/*"],
      "@certifications/modules/*": ["certifications/modules/*"],

      "@auth/root/*": ["auth/*"],
      "@auth/domain/*": ["auth/domain/*"],
      "@auth/application/*": ["auth/application/*"],
      "@auth/infrastructure/*": ["auth/infrastructure/*"],
